{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "no-var-requires": false,
        "max-line-length": [true, 200],
        "align": [false, "parameters", "statements", "arguments", "members", "elements"],
        "one-line": [false],
        "only-arrow-functions": false
    },
    "rulesDirectory": []
}